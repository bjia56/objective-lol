{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Objective-LOL",
  "scopeName": "source.objective-lol",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#numbers"
    },
    {
      "include": "#types"
    },
    {
      "include": "#operators"
    },
    {
      "include": "#identifiers"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.objective-lol",
          "begin": "\\bBTW\\b",
          "end": "$",
          "captures": {
            "0": {
              "name": "punctuation.definition.comment.objective-lol"
            }
          }
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.objective-lol",
          "match": "\\b(HAI\\s+ME\\s+TEH|DIS\\s+TEH|KTHXBAI|KTHX|IZ|NOPE|WHILE|GIVEZ|GIVEZ\\s+UP|MEBBE|OOPS|OOPSIE|ALWAYZ)\\b"
        },
        {
          "name": "keyword.declaration.objective-lol",
          "match": "\\b(VARIABLE|FUNCSHUN|CLAS|KITTEH\\s+OF|AN\\s+OF|NEW)\\b"
        },
        {
          "name": "keyword.modifier.objective-lol",
          "match": "\\b(EVRYONE|MAHSELF|LOCKD|SHARD)\\b"
        },
        {
          "name": "keyword.other.objective-lol",
          "match": "\\b(ITZ|WIT|AN|OF|AS|I|CAN|HAS|A|DO|FROM|TEH)\\b"
        },
        {
          "name": "keyword.control.import.objective-lol",
          "match": "\\b(I\\s+CAN\\s+HAS|FROM)\\b"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.objective-lol",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.objective-lol",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.hex.objective-lol",
          "match": "\\b-?0[Xx][0-9A-Fa-f]+\\b"
        },
        {
          "name": "constant.numeric.octal.objective-lol",
          "match": "\\b-?0[Oo][0-7]+\\b"
        },
        {
          "name": "constant.numeric.decimal.objective-lol",
          "match": "\\b-?\\d+(\\.\\d+)?\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.arithmetic.objective-lol",
          "match": "\\b(MOAR|LES|TIEMZ|DIVIDEZ)\\b"
        },
        {
          "name": "keyword.operator.comparison.objective-lol",
          "match": "\\b(BIGGR\\s+THAN|SMALLR\\s+THAN|SAEM\\s+AS)\\b"
        },
        {
          "name": "keyword.operator.logical.objective-lol",
          "match": "\\b(AN|OR)\\b"
        }
      ]
    },
    "identifiers": {
      "patterns": [
        {
          "name": "constant.language.objective-lol",
          "match": "\\b(YEZ|NO|NOTHIN)\\b"
        },
        {
          "name": "support.type.objective-lol",
          "match": "\\b(INTEGR|DUBBLE|STRIN|BOOL|BUKKIT|BASKIT)\\b"
        },
        {
          "name": "variable.other.objective-lol",
          "match": "\\b[A-Za-z_][A-Za-z0-9_]*\\b"
        }
      ]
    }
  }
}